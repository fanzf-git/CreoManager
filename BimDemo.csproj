  <!-- 定义 ODA SDK 路径变量 -->
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <OdaSdkExePath>$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\exe\vc17_amd64dlldbg</OdaSdkExePath>
    <OdaNativeDllPath>$(SolutionDir)x64\Debug\OdaNative.dll</OdaNativeDllPath>
    <OdaCliWrapperDllPath>$(SolutionDir)x64\Debug\OdaCliWrapper.dll</OdaCliWrapperDllPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <!-- Release 模式使用与 Debug 相同的 NuGet 包，只是子目录不同 -->
    <OdaSdkExePath>$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\exe\vc17_amd64dll</OdaSdkExePath>
    <OdaNativeDllPath>$(SolutionDir)x64\Release\OdaNative.dll</OdaNativeDllPath>
    <OdaCliWrapperDllPath>$(SolutionDir)x64\Release\OdaCliWrapper.dll</OdaCliWrapperDllPath>
  </PropertyGroup>
  <!-- 复制 DLL 文件 -->
  <Target Name="CopyOdaDlls" AfterTargets="Build">
    <Message Text="开始复制 ODA DLL 文件..." Importance="high" />
    <!-- 复制 OdaNative.dll -->
    <Copy SourceFiles="$(OdaNativeDllPath)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="Exists('$(OdaNativeDllPath)')">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedDlls" />
    </Copy>
    <Warning Text="OdaNative.dll 未找到: $(OdaNativeDllPath)" Condition="!Exists('$(OdaNativeDllPath)')" />
    <!-- 复制 OdaCliWrapper.dll -->
    <Copy SourceFiles="$(OdaCliWrapperDllPath)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="Exists('$(OdaCliWrapperDllPath)')">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedDlls" />
    </Copy>
    <Warning Text="OdaCliWrapper.dll 未找到: $(OdaCliWrapperDllPath)" Condition="!Exists('$(OdaCliWrapperDllPath)')" />
    <!-- 复制 ODA SDK 运行时 DLL -->
    <ItemGroup>
      <OdaSdkDlls Include="$(OdaSdkExePath)\*.dll" />
    </ItemGroup>
    <Copy SourceFiles="@(OdaSdkDlls)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="Exists('$(OdaSdkExePath)')">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedDlls" />
    </Copy>
    <Warning Text="ODA SDK DLL 目录未找到: $(OdaSdkExePath)" Condition="!Exists('$(OdaSdkExePath)')" />
    <!-- 复制 ODA SDK 文本资源文件 (.tx) - 从多个位置查找 -->
    <Message Text="开始复制 ODA SDK 文本资源文件 (.tx)..." Importance="high" />
    <!-- 1. 从 exe 目录直接查找 -->
    <ItemGroup>
      <OdaSdkTxFiles Include="$(OdaSdkExePath)\*.tx" Condition="Exists('$(OdaSdkExePath)')" />
    </ItemGroup>
    <Copy SourceFiles="@(OdaSdkTxFiles)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="'@(OdaSdkTxFiles)' != ''">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedTxFiles" />
    </Copy>
    <!-- 2. 从整个 ODA SDK 包中递归查找 -->
    <ItemGroup>
      <OdaSdkTxFilesAll Include="$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\**\*.tx" Condition="'$(Configuration)'=='Debug' and Exists('$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12')" />
      <OdaSdkTxFilesAll Include="$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\**\*.tx" Condition="'$(Configuration)'=='Release' and Exists('$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12')" />
    </ItemGroup>
    <Copy SourceFiles="@(OdaSdkTxFilesAll)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="'@(OdaSdkTxFilesAll)' != ''">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedTxFiles" />
    </Copy>
    <!-- 3. 从 SDK_26.12 完整安装目录查找（如果存在） -->
    <ItemGroup>
      <OdaSdkTxFilesFromSdk Include="E:\Code\cli\SDK_26.12\**\*.tx" Condition="Exists('E:\Code\cli\SDK_26.12')" />
    </ItemGroup>
    <Copy SourceFiles="@(OdaSdkTxFilesFromSdk)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="'@(OdaSdkTxFilesFromSdk)' != ''">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedTxFiles" />
    </Copy>
    <!-- 4. 从 exe 目录的父目录递归查找 -->
    <ItemGroup>
      <OdaSdkTxFilesFromExeParent Include="$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\exe\**\*.tx" Condition="'$(Configuration)'=='Debug' and Exists('$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\exe')" />
      <OdaSdkTxFilesFromExeParent Include="$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\exe\**\*.tx" Condition="'$(Configuration)'=='Release' and Exists('$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\exe')" />
    </ItemGroup>
    <Copy SourceFiles="@(OdaSdkTxFilesFromExeParent)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="'@(OdaSdkTxFilesFromExeParent)' != ''">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedTxFiles" />
    </Copy>
    <!-- 复制 ODA SDK 向量器模块文件 (.txv) -->
    <Message Text="开始复制 ODA SDK 向量器模块文件 (.txv)..." Importance="high" />
    <!-- 1. 从 exe 目录直接查找 .txv -->
    <ItemGroup>
      <OdaSdkTxvFiles Include="$(OdaSdkExePath)\*.txv" Condition="Exists('$(OdaSdkExePath)')" />
    </ItemGroup>
    <Copy SourceFiles="@(OdaSdkTxvFiles)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="'@(OdaSdkTxvFiles)' != ''">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedTxvFiles" />
    </Copy>
    <!-- 2. 从 exe 目录的父目录递归查找 .txv（覆盖 Debug/Release 两种包） -->
    <ItemGroup>
      <OdaSdkTxvFilesFromExeParent Include="$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\exe\**\*.txv" Condition="'$(Configuration)'=='Debug' and Exists('$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\exe')" />
      <OdaSdkTxvFilesFromExeParent Include="$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\exe\**\*.txv" Condition="'$(Configuration)'=='Release' and Exists('$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\exe')" />
    </ItemGroup>
    <Copy SourceFiles="@(OdaSdkTxvFilesFromExeParent)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="'@(OdaSdkTxvFilesFromExeParent)' != ''">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedTxvFiles" />
    </Copy>
    <Message Text="已复制 DLL 文件: @(CopiedDlls)" Importance="normal" Condition="'@(CopiedDlls)' != ''" />
    <Message Text="已复制 .tx 资源文件: @(CopiedTxFiles)" Importance="normal" Condition="'@(CopiedTxFiles)' != ''" />
    <Message Text="已复制 .txv 模块文件: @(CopiedTxvFiles)" Importance="normal" Condition="'@(CopiedTxvFiles)' != ''" />
    <Warning Text="未找到 .tx 资源文件，如果运行时出现相关错误，请检查 ODA SDK 安装或手动复制 .tx 文件到输出目录: $(TargetDir)" Condition="'@(CopiedTxFiles)' == ''" />
    <Warning Text="未找到 .txv 模块文件，如果运行时出现 WinGDI/WinOpenGL 模块加载错误，请检查 ODA SDK 安装或手动复制 .txv 文件到输出目录: $(TargetDir)" Condition="'@(CopiedTxvFiles)' == ''" />
  </Target>
