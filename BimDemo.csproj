  <!-- 定义 ODA SDK 路径变量 -->
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <OdaSdkExePath>$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12\exe\vc17_amd64dlldbg</OdaSdkExePath>
    <OdaNativeDllPath>$(SolutionDir)x64\Debug\OdaNative.dll</OdaNativeDllPath>
    <OdaCliWrapperDllPath>$(SolutionDir)x64\Debug\OdaCliWrapper.dll</OdaCliWrapperDllPath>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <OdaSdkExePath>$(SolutionDir)packages\Kernel_vc17_amd64dll_26.12_Release\exe\vc17_amd64dll</OdaSdkExePath>
    <OdaNativeDllPath>$(SolutionDir)x64\Release\OdaNative.dll</OdaNativeDllPath>
    <OdaCliWrapperDllPath>$(SolutionDir)x64\Release\OdaCliWrapper.dll</OdaCliWrapperDllPath>
  </PropertyGroup>
  <!-- 复制 DLL 文件 -->
  <Target Name="CopyOdaDlls" AfterTargets="Build">
    <Message Text="开始复制 ODA DLL 文件..." Importance="high" />
    <!-- 复制 OdaNative.dll -->
    <Copy SourceFiles="$(OdaNativeDllPath)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="Exists('$(OdaNativeDllPath)')">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedDlls" />
    </Copy>
    <Warning Text="OdaNative.dll 未找到: $(OdaNativeDllPath)" Condition="!Exists('$(OdaNativeDllPath)')" />
    <!-- 复制 OdaCliWrapper.dll -->
    <Copy SourceFiles="$(OdaCliWrapperDllPath)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="Exists('$(OdaCliWrapperDllPath)')">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedDlls" />
    </Copy>
    <Warning Text="OdaCliWrapper.dll 未找到: $(OdaCliWrapperDllPath)" Condition="!Exists('$(OdaCliWrapperDllPath)')" />
    <!-- 复制 ODA SDK 运行时 DLL -->
    <ItemGroup>
      <OdaSdkDlls Include="$(OdaSdkExePath)\*.dll" />
    </ItemGroup>
    <Copy SourceFiles="@(OdaSdkDlls)" DestinationFolder="$(TargetDir)" SkipUnchangedFiles="true" Condition="Exists('$(OdaSdkExePath)')">
      <Output TaskParameter="CopiedFiles" ItemName="CopiedDlls" />
    </Copy>
    <Warning Text="ODA SDK DLL 目录未找到: $(OdaSdkExePath)" Condition="!Exists('$(OdaSdkExePath)')" />
    <Message Text="已复制 DLL 文件: @(CopiedDlls)" Importance="normal" Condition="'@(CopiedDlls)' != ''" />
  </Target>
